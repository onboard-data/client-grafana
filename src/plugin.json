{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Onboard Data Connector",
  "id": "onboarddata-portal-datasource",
  "metrics": true,
  "info": {
    "description": "",
    "author": {
      "name": "Onboard Data",
      "url": "https://onboarddata.io"
    },
    "keywords": ["bacnet", "modbus", "bas"],
    "logos": {
      "small": "img/onboard-logo.png",
      "large": "img/onboard-logo.png"
    },
    "links": [
      {
        "name": "Onboard Website",
        "url": "https://onboarddata.io"
      },
      {
        "name": "Repository",
        "url": "https://github.com/onboard-data/client-grafana"
      },
      {
        "name": "License",
        "url": "https://github.com/onboard-data/client-grafana/blob/dev/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  },
  "routes": [
    {
      "path": "portal",
      "url": "https://api.onboarddata.io",
      "headers": [
        {
          "name": "X-OB-Api",
          "content": "{{ .SecureJsonData.apiKey }}"
        }
      ]
    }
  ]
}
